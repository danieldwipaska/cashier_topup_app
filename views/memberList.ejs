<script>
  async function fetchMemberData(memberId) {
    try {
      const data = await fetch(`http://localhost:3000/member/details/${memberId}`);

      const response = await data.json();

      const customerId = document.querySelector('.customer-id');
      const barcode = document.querySelector('.barcode');
      const name = document.querySelector('.name');
      const identityNumber = document.querySelector('.identity-number');
      const birthDate = document.querySelector('.birth-date');
      const email = document.querySelector('.email');
      const address = document.querySelector('.address');
      const instagram = document.querySelector('.instagram');
      const facebook = document.querySelector('.facebook');
      const twitter = document.querySelector('.twitter');
      const activeStatus = document.querySelector('.active-status');
      const form = document.querySelector('form');

      customerId.value = response.customer_id;
      barcode.value = response.barcode;
      name.value = response.fullname;
      identityNumber.value = response.identity_number;
      birthDate.value = response.birth_date;
      email.value = response.email;
      address.value = response.address;
      instagram.value = response.instagram;
      facebook.value = response.facebook;
      twitter.value = response.twitter;
      form.setAttribute('action', `/member/details/${response.id}`);

      if (response.is_active) {
        activeStatus.innerHTML = `<div class="form-check form-switch">
            <input class="form-check-input" type="checkbox" role="switch" id="flexSwitchCheckChecked" name="isActive" checked />
            <label class="form-check-label" for="flexSwitchCheckChecked">Active</label>
          </div>`;
      } else {
        activeStatus.innerHTML = `<div class="form-check form-switch">
            <input class="form-check-input" type="checkbox" role="switch" id="flexSwitchCheckChecked" name="isActive" />
            <label class="form-check-label" for="flexSwitchCheckChecked">Active</label>
          </div>`;
      }
    } catch (error) {
      console.log(error);
    }
  }

  const editButtons = document.querySelectorAll('.edit-button');

  editButtons.forEach((editButton) => {
    editButton.addEventListener('click', (e) => {
      const memberId = editButton.querySelector('input').value;

      // console.log(memberId);
      fetchMemberData(memberId);
    });
  });
</script>
