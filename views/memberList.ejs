<!-- Modal -->
<div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog modal-lg">
    <div class="modal-content">
      <form action="#" method="post">
        <div class="modal-header">
          <h5 class="modal-title" id="exampleModalLabel">Member Data</h5>
          <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
        </div>
        <div class="modal-body">
          <label class="form-label">Customer Details</label>
          <div class="input-group mb-3">
            <span class="input-group-text" id="customerId">Phone*</span>
            <input type="text" class="form-control customer-id" aria-label="Sizing example input" aria-describedby="customerId" name="customerId" required />
            <span class="input-group-text" id="barcode">Card</span>
            <input type="text" class="form-control barcode" aria-label="Sizing example input" aria-describedby="barcode" name="barcode" readonly style="background-color: rgb(211, 211, 211)" />
          </div>
          <div class="input-group mb-3">
            <span class="input-group-text" id="name">Fullname*</span>
            <input type="text" class="form-control name" aria-label="Sizing example input" aria-describedby="name" name="name" required />
          </div>
          <div class="input-group mb-3">
            <span class="input-group-text" id="identityNumber">Identity Number*</span>
            <input type="text" class="form-control identity-number" aria-label="Sizing example input" aria-describedby="identityNumber" name="identityNumber" required />
            <span class="input-group-text" id="birthDate">Tanggal Lahir*</span>
            <input type="date" class="form-control birth-date" aria-label="Sizing example input" aria-describedby="birthDate" name="birthDate" required />
          </div>
          <div class="input-group mb-3">
            <span class="input-group-text" id="email">Email*</span>
            <input type="email" class="form-control email" aria-label="Sizing example input" aria-describedby="email" name="email" required />
          </div>
          <div class="input-group mb-3">
            <span class="input-group-text" id="address">Address</span>
            <input type="text" class="form-control address" aria-label="Sizing example input" aria-describedby="address" name="address" required />
          </div>
          <label class="form-label">Social Media</label>
          <div class="input-group mb-3">
            <span class="input-group-text" id="instagram">https://instagram.com/</span>
