<div class="container-md my-3">
  <div class="row my-2">
    <div class="col-md-8 mx-auto">
      <div class="card mb-3">
        <div class="card-header text-white bg-danger">ERROR</div>
        <div class="card-body">
          <h5 class="card-title">Progress Failed Before finished.</h5>
          <h6>Details:</h6>

          <% success.forEach(element => { %>
          <div>
            <svg viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg" style="max-width: 45px">
              <g id="SVGRepo_bgCarrier" stroke-width="0"></g>
              <g id="SVGRepo_tracerCarrier" stroke-linecap="round" stroke-linejoin="round"></g>
              <g id="SVGRepo_iconCarrier"><path d="M5 14L8.23309 16.4248C8.66178 16.7463 9.26772 16.6728 9.60705 16.2581L18 6" stroke="#2ec27e" stroke-width="2" stroke-linecap="round"></path></g>
            </svg>
            <p class="card-text" style="display: inline"><%= element %></p>
          </div>

          <% }); %> <% failed.forEach(element => { %>
          <div>
            <svg fill="#e01b24" viewBox="0 0 32 32" xmlns="http://www.w3.org/2000/svg" style="max-width: 45px">
              <g id="SVGRepo_bgCarrier" stroke-width="0"></g>
              <g id="SVGRepo_tracerCarrier" stroke-linecap="round" stroke-linejoin="round"></g>
              <g id="SVGRepo_iconCarrier">
                <path
                  d="M18.8,16l5.5-5.5c0.8-0.8,0.8-2,0-2.8l0,0C24,7.3,23.5,7,23,7c-0.5,0-1,0.2-1.4,0.6L16,13.2l-5.5-5.5 c-0.8-0.8-2.1-0.8-2.8,0C7.3,8,7,8.5,7,9.1s0.2,1,0.6,1.4l5.5,5.5l-5.5,5.5C7.3,21.9,7,22.4,7,23c0,0.5,0.2,1,0.6,1.4 C8,24.8,8.5,25,9,25c0.5,0,1-0.2,1.4-0.6l5.5-5.5l5.5,5.5c0.8,0.8,2.1,0.8,2.8,0c0.8-0.8,0.8-2.1,0-2.8L18.8,16z"
                ></path>
              </g>
            </svg>
            <p class="card-text" style="display: inline"><%= element %></p>
          </div>

          <% }); %>
          <br />
          <h6 class="text-sm"><%= message %></h6>
        </div>
      </div>
    </div>
  </div>
  <div class="row my-3">
    <div class="col-md-8 mx-auto">
      <button class="btn btn-search" onclick="history.back()"><span class="mx-2">Back</span></button>
    </div>
  </div>
</div>
