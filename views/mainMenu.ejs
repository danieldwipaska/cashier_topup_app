      let totalPurchasesMoka = Intl.NumberFormat('en-US').format(data.mokaPurchases);
      let totalRefundsMoka = Intl.NumberFormat('en-US').format(data.mokaRefunds);
      let totalPurchasesBcm = Intl.NumberFormat('en-US').format(data.bcm);

      totalPaymentFromMoka.innerHTML = totalPurchasesMoka;
      totalRefundFromMoka.innerHTML = totalRefundsMoka;
      totalPaymentFromBcm.innerHTML = totalPurchasesBcm;
    } catch (error) {
      totalPaymentFromMoka.innerHTML = 'Not Available';
      totalRefundFromMoka.innerHTML = 'Not Available';
      totalPaymentFromBcm.innerHTML = 'Not Available';
    }
  }

