  async function fetchTotalPayment() {
    try {
      const response = await fetch('https://baharimalangirishpub-app.com/analysis/payment');
      const data = await response.json();

      let totalPayment = 0;
      data.forEach((element) => {
        totalPayment = totalPayment + element.payment;
      });

      totalPayment = Intl.NumberFormat('en-US').format(totalPayment);

      totalPaymentData.innerHTML = totalPayment;
    } catch (error) {
      totalPaymentData.innerHTML = 'Not Available';
    }
  }

  async function fetchTotalPaymentFromMoka() {
